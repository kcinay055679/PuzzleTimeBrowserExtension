{
  "name": "PuzzleTime Angular",
  "version": "2.0",
  "description": "You can take notes, which includes text, lists, links & images. We can export our notes in the form of html..",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "storage"
  ],
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["background.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/puzzleTime.png",
      "38": "assets/puzzleTime.png"
    },
    "default_popup": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  }

}
