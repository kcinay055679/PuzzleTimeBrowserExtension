{
  "name": "PuzzleTime QuickChange",
  "version": "2.0",
  "description": "You can use this extensions to quick switch between different presets of task",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "storage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://pitc-puzzletime-int.ocp.cloudscale.puzzle.ch/ordertimes/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "19": "assets/puzzleTime.png",
      "38": "assets/puzzleTime.png"
    },
    "default_popup": "index.html"
  }
}
